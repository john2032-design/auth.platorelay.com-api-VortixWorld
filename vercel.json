{
  "version": 2,
  "builds": [
    {
      "src": "api/platorelay.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/platorelay.js"
    }
  ],
  "functions": {
    "api/platorelay.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}