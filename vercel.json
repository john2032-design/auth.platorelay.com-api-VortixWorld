{
  "builds": [
    {
      "src": "api/platorelay.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/platorelay.js"
    }
  ],
  "functions": {
    "api/platorelay.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
